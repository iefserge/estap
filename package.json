{
  "name": "estap",
  "version": "0.0.0",
  "description": "JavaScript TAP test framework",
  "main": "dist/index.js",
  "scripts": {
    "build": "mkdir -p dist && babel -d dist lib",
    "lint": "eslint .",
    "test:browser": "NODE_ENV=test browserify -t babelify test/unit/index.js | tape-run",
    "test": "NODE_ENV=test nyc --require babel-core/register tape test/unit/index.js test/integration/index.js && nyc check-coverage --lines 100 --functions 100 --branches 100 --statements 100 && npm run lint",
    "test:unit": "NODE_ENV=test babel-node test/unit/index.js",
    "test:integration": "NODE_ENV=test babel-node test/integration/index.js",
    "report": "nyc report --reporter=lcov && open coverage/lcov-report/index.html",
    "coverage": "nyc report --reporter=lcov && cat ./coverage/lcov.info | coveralls",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:iefserge/estap.git"
  },
  "author": "Sergii Iefremov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iefserge/estap/issues"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  },
  "homepage": "http://iefserge.com",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-plugin-istanbul": "^1.0.3",
    "babel-plugin-source-map-support-for-6": "0.0.5",
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-config-airbnb-base": "^5.0.1",
    "eslint-plugin-import": "^1.12.0",
    "nyc": "^7.1.0",
    "source-map-support": "^0.4.2",
    "tape": "^4.6.0",
    "tape-run": "^2.1.4"
  },
  "dependencies": {
    "bluebird": "^3.4.1",
    "deep-equal": "^1.0.1",
    "es6-map": "^0.1.4",
    "es6-set": "^0.1.4",
    "object-assign": "^4.1.0",
    "object-inspect": "^1.2.1",
    "serialize-access": "^1.0.0",
    "yamljs": "^0.2.8"
  }
}
